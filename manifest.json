{
    "name": "Discord Picture in Picture",
    "version": "1.0",
    "description": "Picture in Picture discord streams",
    "manifest_version": 2,
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "logo.png"
    },
    "content_scripts": [
      {
        "matches": [
          "https://discord.com/*"
        ],
        "js": [
          "main.js"
        ]
      }
    ]
  }
  